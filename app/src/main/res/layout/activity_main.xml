<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="testing.gps_service.MainActivity">


    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/background" />

    <TableLayout
        android:id="@+id/TabelForButtons"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_marginTop="150dp"
        android:layout_centerHorizontal="true"
        android:stretchColumns="*">

    </TableLayout>


    <Button
        android:id="@+id/button1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="110dp"
        android:background="@color/Quiet" />

    <Button
        android:id="@+id/button2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="150dp"
        android:background="@color/Normal" />

    <Button
        android:id="@+id/button3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="190dp"
        android:background="@color/Crowded" />

    <Button
        android:id="@+id/button4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="230dp"
        android:background="@color/Very_crowded" />

    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="39dp"
        android:layout_marginLeft="39dp"
        android:layout_marginBottom="32dp"
        android:text="Start Service" />

    <Button
        android:id="@+id/stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/start"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="41dp"
        android:layout_marginRight="41dp"
        android:text="Stop Service" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="311dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="Festival Map"
        android:textAlignment="center"
        android:textSize="36sp"

        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="272dp"
        android:layout_height="43dp"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="8dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="Lowlands"
        android:textAlignment="center"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/stage_image"
        android:layout_width="55dp"
        android:layout_height="110dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="129dp"
        android:layout_marginBottom="8dp"
        app:srcCompat="@drawable/stage_symbol"/>

    <ImageView
        android:id="@+id/food_image"
        android:layout_width="55dp"
        android:layout_height="110dp"
        android:layout_marginTop="240dp"
        android:layout_marginLeft="230dp"
        android:layout_marginBottom="8dp"
        app:srcCompat="@drawable/eten_symbol"
        />

    <ImageView
        android:id="@+id/bar_image"
        android:layout_width="55dp"
        android:layout_height="110dp"
        android:layout_marginTop="160dp"
        android:layout_marginLeft="62dp"
        android:layout_marginBottom="140dp"
        app:srcCompat="@drawable/bar_symbol"
        />

    <ImageView
        android:id="@+id/theater_image"
        android:layout_width="55dp"
        android:layout_height="110dp"
        android:layout_marginLeft="145dp"
        android:layout_marginTop="282dp"
        android:layout_marginBottom="60dp"
        app:srcCompat="@drawable/theater_symbol" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="115dp"
        android:text="Less than (...) people present"
        android:textColor="@color/White"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="155dp"
        android:text="More than (...) people present"
        android:textColor="@color/White"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="195dp"
        android:text="More than (...) people present"
        android:textColor="@color/White"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView5"
        android:layout_alignLeft="@+id/textView5"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="100dp"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="235dp"
        android:text="More than (...) people present"
        android:textColor="@color/White"
        android:textSize="18sp" />




</RelativeLayout>